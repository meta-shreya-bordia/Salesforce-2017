<apex:page standardController="Student__c">    
	<apex:pageBlock >
    	<apex:pageBlockTable value="{!Student__c}" var="student">
            <apex:column headerValue="{!$Label.First_Name}" value="{!student.First_Name__c}" />
            <apex:column headerValue="{!$Label.Last_Name}" value="{!student.Last_Name__c}" />
            <apex:column headerValue="{!$Label.Class}" value="{!student.Class__r.Name}" />
        </apex:pageBlockTable>
    </apex:pageBlock>                
</apex:page>