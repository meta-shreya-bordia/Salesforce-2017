    <validationRules>
        <fullName>DateIsWeekDay</fullName>
        <active>true</active>
        <description>Validates that the value of a custom date field is a weekday (not Saturday or Sunday).</description>
        <errorConditionFormula>CASE(MOD( Weekdate__c - DATE(1900, 1, 7), 7),0, 0, 6, 0, 1) = 0
 ||
CASE(MOD( Weekdate__c - DATE(1900, 1, 7), 7),0, 0, 6, 0, 1) = 6</errorConditionFormula>
        <errorDisplayField>Weekdate__c</errorDisplayField>
        <errorMessage>Work only weekdays. No Saturday Sunday value accepted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidDOB</fullName>
        <active>true</active>
        <errorConditionFormula>DOB__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>DOB__c</errorDisplayField>
        <errorMessage>Student DOB should not be greater than TODAY</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidMixPct</fullName>
        <active>true</active>
        <errorConditionFormula>OR
(
Mix_Pct__c &lt; 0.00, 
Mix_Pct__c &gt; 100.00
)</errorConditionFormula>
        <errorDisplayField>Mix_Pct__c</errorDisplayField>
        <errorMessage>Percentage must be in valid range (0-100%)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidMultiple</fullName>
        <active>true</active>
        <description>Validates that a custom field called Multiple of 5 is a multiple of five.</description>
        <errorConditionFormula>( Multiple_of_5__c  / 5 )  &lt;&gt; 0</errorConditionFormula>
        <errorDisplayField>Multiple_of_5__c</errorDisplayField>
        <errorMessage>Number must be a multiple of five.</errorMessage>
    </validationRules>
